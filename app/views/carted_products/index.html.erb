<h1>Checkout Page</h1>

<div class="well"> 
  <% @carted_product.each do |carted_product| %>
    <h2><%= carted_product.stamp.name %></h2>
    <h2><%= number_to_currency(carted_product.stamp.price) %></h2>
    <h2><%= carted_product.quantity %></h2>
    <%= link_to "remove", "/carted_products/#{carted_product.id}", method: :delete %>
  <% end %>
</div>


<%= link_to "Buy Now", "/orders", method: :post, class: "btn btn-success" %>







