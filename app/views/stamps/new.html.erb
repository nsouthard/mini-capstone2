<h1>Add to the Collection</h1>

<%= form_tag "/stamps", method: :post do %>
  <div>
    Stamp Name: <input type="text" name="name">
  </div>
 <div>
    Year: <input type="text" name="year">
  </div>
  <div>
    Image URL: <input type="text" name="image">
  </div>
   <div>
    Price: <input type="integer" name="price">
  </div>
   <div>
    Description: <input type="text" name="description">
  </div>
  <input type="submit" value="Add Stamp">

<% end %>
